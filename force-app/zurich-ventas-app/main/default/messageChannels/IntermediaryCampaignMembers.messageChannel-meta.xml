<?xml version="1.0" encoding="UTF-8" ?>
<LightningMessageChannel xmlns="http://soap.sforce.com/2006/04/metadata">
    <masterLabel>IntermediaryCampaignMembers</masterLabel>
    <isExposed>true</isExposed>
    <description>Canal para pasar información entre la tabla de Campañas y Miembros de Campaña.</description>
     <lightningMessageFields>
        <fieldName>statusRefreshed</fieldName>
        <description>Indica si se ha modificado el valor del filtro de estado de la tabla de Campañas</description>
    </lightningMessageFields>
    <lightningMessageFields>
        <fieldName>campaignStatus</fieldName>
        <description>Estado de la campaña seleccionada.</description>
    </lightningMessageFields>
    <lightningMessageFields>
         <fieldName>campaignId</fieldName>
        <description>Id de la Campaña seleccionada</description>
     </lightningMessageFields>
     <lightningMessageFields>
        <fieldName>campaignStartDate</fieldName>
        <description>Fecha de inicio de la campaña</description>
     </lightningMessageFields>
     <lightningMessageFields>
        <fieldName>campaignType</fieldName>
        <description>Tipo de campaña</description>
     </lightningMessageFields>
</LightningMessageChannel>