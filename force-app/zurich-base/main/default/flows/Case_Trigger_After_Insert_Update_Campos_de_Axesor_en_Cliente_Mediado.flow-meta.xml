    <formulas>
        <name>ActualizarClienteMediadoControl</name>
        <dataType>Boolean</dataType>
        <expression>AND(
      OR(
		ISNEW(),
		ISCHANGED({!$Record.MediatedCustomer__c}),
		ISCHANGED({!$Record.RobotNif__c}),
        ISCHANGED({!$Record.RecordTypeId})
	),
	NOT(ISNULL({!$Record.MediatedCustomer__c})),
	NOT(ISNULL({!$Record.RobotNif__c})),
	{!$Record.RobotNif__c} = {!$Record.MediatedCustomer__r.NationalId__c})</expression>
    </formulas>
    <formulas>
        <name>AxesorBillingStreet</name>
        <dataType>String</dataType>
        <expression>IF(
AND(
NOT(ISNULL({!$Record.RobotCompanyRoadType__c})),
NOT(ISNULL({!$Record.RobotCompanyRoadName__c}))),
IF(NOT(ISNULL({!$Record.RobotCompanyAdditionalInformation__c})),
{!$Record.RobotCompanyRoadType__c}+&apos; &apos;+{!$Record.RobotCompanyRoadName__c}+&apos; &apos;+{!$Record.RobotCompanyRoadNumber__c}+&apos; &apos;+{!$Record.RobotCompanyAdditionalInformation__c}, {!$Record.RobotCompanyRoadType__c}+&apos; &apos;+{!$Record.RobotCompanyRoadName__c}+&apos; &apos;+{!$Record.RobotCompanyRoadNumber__c}),&apos;&apos;)</expression>
    </formulas>
    <formulas>
        <name>AxesorCNAECode</name>
        <dataType>String</dataType>
        <expression>IF(
AND(
LEN({!$Record.RobotCompanyActivityCnaeCode__c}) &lt; 5,
!BEGINS({!$Record.RobotCompanyActivityCnaeCode__c}, &apos;0&apos;)),
LPAD({!$Record.RobotCompanyActivityCnaeCode__c}, 5, &apos;0&apos;),
{!$Record.RobotCompanyActivityCnaeCode__c})</expression>
    </formulas>
    <formulas>
        <description>Convertimos tipo texto a numero</description>
        <name>AxesorLatitude</name>
        <dataType>Number</dataType>
        <expression>VALUE(SUBSTITUTE({!$Record.RobotCompanyLatitude__c}, &quot;,&quot;, &quot;.&quot;))</expression>
        <scale>2</scale>
    </formulas>
    <formulas>
        <name>AxesorLongitude</name>
        <dataType>Number</dataType>
        <expression>VALUE(SUBSTITUTE({!$Record.RobotCompanyLongitude__c}, &quot;,&quot;, &quot;.&quot;))</expression>
        <scale>2</scale>
    </formulas>
    <formulas>
        <name>FuncionalidadCIFNIFControl</name>
        <dataType>Boolean</dataType>
        <expression>( OR(
		ISCHANGED({!$Record.MediatedCustomer__c}),
		ISCHANGED({!$Record.RobotNif__c}),
                 ISCHANGED({!$Record.CIFNIF__c}),
                NOT(ISNULL({!$Record.MediatedCustomer__c})),
	        NOT(ISNULL({!$Record.RobotNif__c}))
	)&amp;&amp; {!$Record.RecordType.DeveloperName}==&apos;USPInquiry&apos; &amp;&amp; NOT(ISPICKVAL({!$Record.Status},&apos;Cerrado&apos;)) &amp;&amp;NOT(ISPICKVAL({!$Record.Status},&apos;Combinado&apos;)) )</expression>
    </formulas>
