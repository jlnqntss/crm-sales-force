    <formulas>
        <name>AxesorBillingStreet</name>
        <dataType>String</dataType>
        <expression>IF(
AND(
NOT(ISNULL({!$Record.RobotCompanyRoadType__c})),
NOT(ISNULL({!$Record.RobotCompanyRoadName__c}))),
IF(NOT(ISNULL({!$Record.RobotCompanyAdditionalInformation__c})),
{!$Record.RobotCompanyRoadType__c}+&apos; &apos;+{!$Record.RobotCompanyRoadName__c}+&apos; &apos;+{!$Record.RobotCompanyRoadNumber__c}+&apos; &apos;+{!$Record.RobotCompanyAdditionalInformation__c}, {!$Record.RobotCompanyRoadType__c}+&apos; &apos;+{!$Record.RobotCompanyRoadName__c}+&apos; &apos;+{!$Record.RobotCompanyRoadNumber__c}),&apos;&apos;)</expression>
    </formulas>
    <formulas>
        <name>AxesorCNAECode</name>
        <dataType>String</dataType>
        <expression>IF(
AND(
LEN({!$Record.RobotCompanyActivityCnaeCode__c}) &lt; 5,
!BEGINS({!$Record.RobotCompanyActivityCnaeCode__c}, &apos;0&apos;)),
LPAD({!$Record.RobotCompanyActivityCnaeCode__c}, 5, &apos;0&apos;),
{!$Record.RobotCompanyActivityCnaeCode__c})</expression>
    </formulas>
    <formulas>
