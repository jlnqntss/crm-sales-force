<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Alta_CIMA__c</fullName>
    <description>Campo que comprueba si EIAC(Status) es Activo, si lo es, marca el check.</description>
    <externalId>false</externalId>
    <formula>ISPICKVAL( EIACStatus__c , &quot;Activo&quot;)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Alta CIMA</label>
    <trackHistory>false</trackHistory>
    <type>Checkbox</type>
</CustomField>
