{
  "packageDirectories": [
    {
      "path": "force-app/zurich-base",
      "default": true
    },
    {
      "path": "force-app/zurich-sdm-app",
      "default": false
    },
    {
      "path": "force-app/zurich-base-iam",
      "default": false
    },
    {
      "path": "force-app/zurich-klinc-app",
      "default": false
    },
    {
      "path": "force-app/zurich-ocs-app",
      "default": false
    },
    {
      "path": "force-app/zurich-ze-app",
      "default": false
    },
    {
      "path": "force-app/zurich-base-security",
      "default": false
    }
  ],
  "plugins": {
    "nts1": {
      "hooks": {
        "profiles": {
          "pullChanges": true,
          "reconcileWithProject": false,
          "reconcileWithOrg": true
        },
        "translations": {
          "pullObjectTranslations": true
        }
      }
    }
  },
  "namespace": "",
  "sfdcLoginUrl": "https://login.salesforce.com",
  "sourceApiVersion": "52.0"
}
